#Round1 output directory
gff3_merge -s -d round1_ms_master_datastore_index.log > ms_rnd1.all.maker.gff
gff3_merge -n -s -d round1_ms_master_datastore_index.log > ms_rnd1.all.maker.noseq.gff
# transcript alignments
awk '{ if ($2 == "est2genome") print $0 }' ms_rnd1.all.maker.noseq.gff > ms_rnd1.all.maker.est2genome.gff
# protein alignments
awk '{ if ($2 == "protein2genome") print $0 }' ms_rnd1.all.maker.noseq.gff > ms_rnd1.all.maker.protein2genome.gff
# repeat alignments
awk '{ if ($2 ~ "repeat") print $0 }' ms_rnd1.all.maker.noseq.gff > ms_rnd1.all.maker.repeats.gff
